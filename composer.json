{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	"require": {
		"laravel/framework": "4.2.*"
	},
	"require-dev": {
    	"way/generators": "2.*"
	},
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/models",
			"app/database/migrations",
			"app/database/seeds",
			"app/filters",
			"app/tests/TestCase.php"
		],
		"files": [
			"app/src/mechanic/paperclip/support/helpers.php"
		],
		"psr-4": {
			"ViewHelpers\\": "app/viewhelpers",
			"API\\JSON\\": "app/jsonapi",
			"PaperClip\\": "app/src/mechanic/paperclip",
			"PaperClip\\Vendor\\": "app/src/mechanic/paperclip/vendor",
			"Jacopo\\Bootstrap3Table\\": "app/src/vendor/jacopo/bootstrap-3-table-generator/src/Jacopo/Bootstrap3Table"
        },
        "_comment-psr-0": {"PaperClip": "app/src"}
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "stable"
}
